{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        // "moduleResolution": "node",
        "sourceMap": true,
        "declaration": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "lib": [
            "es6",
            "es2015",
            "es2016",
            "es2017",
            "dom"
        ]
        // "outDir": ""
    },
    "buildOnSave": false,
    "compileOnSave": false,
    "include": [
        "src/**/*"
    ],
    "exclude": [
        // "node_modules"
    ],
    "files": [
        "*"
    ],
    "types": [
        "node"
    ],
    "awesomeTypescriptLoaderOptions": {
        "useWebpackText": true
    }
}